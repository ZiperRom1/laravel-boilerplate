{
    "tags"     : {
        "allowUnknownTags": true
    },
    "source"   : {
        "include"       : ["public/js", "readme.md"],
        "exclude"       : ["js/app.build.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins"  : [],
    "templates": {
        "cleverLinks"          : false,
        "monospaceLinks"       : false,
        "outputSourcePath"     : true,
        "footer"               : "",
        "copyright"            : "DocStrap Copyright Â© 2012-2014 The contributors to the JSDoc3 and DocStrap projects.",
        "protocol"             : "html://",
        "systemName"           : "Laravel boilerplate",
        "navType"              : "inline",
        "theme"                : "journal",
        "linenums"             : true,
        "collapseSymbols"      : false,
        "inverseNav"           : true,
        "outputSourceFiles"    : true,
        "dateFormat"           : "YYYY-MM-DD H:mm:ss",
        "highlightTutorialCode": false
    },
    "opts"     : {
        "destination": "storage/app/public/jsDoc"
    }
}
